<?xml version="1.0" encoding="us-ascii"?>
<XML ID="EQInterfaceDefinitionLanguage">
    <Schema xmlns="EverQuestData" xmlns:dt="EverQuestDataTypes" />

    <!-- -->

    <Button item="OW_ALL_QuestImage">
        <ScreenID>OW_ALL_QuestImage</ScreenID>
        <Template>DefaultImageOW</Template>

        <AutoStretch>true</AutoStretch>
        <LeftAnchorToLeft>true</LeftAnchorToLeft>
        <RightAnchorToLeft>false</RightAnchorToLeft>
        <TopAnchorToTop>true</TopAnchorToTop>
        <BottomAnchorToTop>false</BottomAnchorToTop>
    </Button>

    <Label item="OW_ALL_TitleLabel">
        <ScreenID>OW_ALL_TitleLabel</ScreenID>
        <Font>5</Font>
        <NoWrap>true</NoWrap>
        <ResizeHeightToText>true</ResizeHeightToText>

        <TextColor>
            <R>0</R>
            <G>0</G>
            <B>0</B>
        </TextColor>

        <AutoStretchHorizontal>true</AutoStretchHorizontal>
        <LeftAnchorToLeft>true</LeftAnchorToLeft>
        <RightAnchorToLeft>false</RightAnchorToLeft>

        <LeftAnchorOffset>2</LeftAnchorOffset>
        <RightAnchorOffset>2</RightAnchorOffset>
        <TopAnchorOffset>1</TopAnchorOffset>
    </Label>

    <LayoutBox item="OW_ALL_TitleImageLayout">
        <AutoStretch>true</AutoStretch>
        <LeftAnchorToLeft>true</LeftAnchorToLeft>
        <RightAnchorToLeft>true</RightAnchorToLeft>
        <TopAnchorToTop>true</TopAnchorToTop>
        <BottomAnchorToTop>false</BottomAnchorToTop>

        <RightAnchorOffset>320</RightAnchorOffset>

        <Pieces>OW_ALL_QuestImage</Pieces>
        <Pieces>OW_ALL_TitleLabel</Pieces>
    </LayoutBox>

    <!-- -->

    <Label item="OW_ALL_DurationValue">
        <ScreenID>OW_ALL_DurationValue</ScreenID>
        <PrependText>Duration: </PrependText>
        <NoWrap>true</NoWrap>
        <ResizeHeightToText>true</ResizeHeightToText>
        <Size>
            <CX>128</CX>
        </Size>
    </Label>

    <Label item="OW_ALL_SuccessValue">
        <ScreenID>OW_ALL_SuccessValue</ScreenID>
        <PrependText>Success: </PrependText>
        <NoWrap>true</NoWrap>
        <ResizeHeightToText>true</ResizeHeightToText>
        <Size>
            <CX>128</CX>
        </Size>
    </Label>

    <Button item="OW_ALL_StartButton">
        <TooltipReference>Click to start this Overseer Quest</TooltipReference>
        <ScreenID>OW_ALL_StartButton</ScreenID>
        <Template>BDT_Normal</Template>
        <Text>Start Quest!</Text>
        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <Button item="OW_ALL_CollectRewardButton">
        <TooltipReference>Click to complete this Overseer Quest and receive your rewards</TooltipReference>
        <ScreenID>OW_ALL_CollectRewardButton</ScreenID>
        <Text>Collect Reward!</Text>
        <Template>BDT_Normal</Template>
        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <Button item="OW_ALL_FinishEarlyButton">
        <TooltipReference>Click to instantly complete this Overseer Quest</TooltipReference>
        <ScreenID>OW_ALL_FinishEarlyButton</ScreenID>
        <Template>BDT_ButtonStation</Template>
        <Text>Finish Now!</Text>
        <TextAlignCenter>false</TextAlignCenter>
        <TextOffsetX>5</TextOffsetX>

        <TextColor>
            <R>255</R>
            <G>233</G>
            <B>129</B>
        </TextColor>

        <DecalOffset>
            <X>80</X>
            <Y>2</Y>
        </DecalOffset>
        <DecalSize>
            <CX>16</CX>
            <CY>16</CY>
        </DecalSize>

        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <Button item="OW_ALL_PreviewRewardButton">
        <TooltipReference>Click to preview the rewards for this Overseer Quest</TooltipReference>
        <ScreenID>OW_ALL_PreviewRewardButton</ScreenID>
        <Template>BDT_Normal</Template>
        <Text>Preview Reward</Text>
        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <Button item="OW_ALL_CompleteQuestButton">
        <TooltipReference>Click to complete this Overseer Quest</TooltipReference>
        <ScreenID>OW_ALL_CompleteQuestButton</ScreenID>
        <Template>BDT_Normal</Template>
        <Text>Complete Quest!</Text>
        <Style_Checkbox>false</Style_Checkbox>
        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <Button item="OW_ALL_AbandonButton">
        <TooltipReference>Click to abandon this Overseer Quest</TooltipReference>
        <ScreenID>OW_ALL_AbandonButton</ScreenID>
        <Template>BDT_Normal</Template>
        <Text>Abandon</Text>
        <Size>
            <CX>100</CX>
            <CY>20</CY>
        </Size>
    </Button>

    <TileLayoutBox item="OW_ALL_QuestStatsLayout">
        <HorizontalFirst>false</HorizontalFirst>

        <AutoStretch>true</AutoStretch>
        <LeftAnchorToLeft>true</LeftAnchorToLeft>
        <RightAnchorToLeft>false</RightAnchorToLeft>
        <TopAnchorToTop>true</TopAnchorToTop>
        <BottomAnchorToTop>false</BottomAnchorToTop>

        <LeftAnchorOffset>252</LeftAnchorOffset>

        <Spacing>2</Spacing>
        <SecondarySpacing>2</SecondarySpacing>

        <Pieces>OW_ALL_DurationValue</Pieces>
        <Pieces>OW_ALL_SuccessValue</Pieces>

        <Pieces>OW_ALL_StartButton</Pieces>
        <Pieces>OW_ALL_CollectRewardButton</Pieces>
        <Pieces>OW_ALL_FinishEarlyButton</Pieces>

        <Pieces>OW_ALL_PreviewRewardButton</Pieces>
        <Pieces>OW_ALL_CompleteQuestButton</Pieces>
        <Pieces>OW_ALL_AbandonButton</Pieces>
    </TileLayoutBox>

    <Screen item="OW_ALL_QuestTileScreen">
        <ScreenID>OW_ALL_QuestTileScreen</ScreenID>
		<DrawTemplate>WDT_Inner</DrawTemplate>
        <Style_Border>true</Style_Border>
        <Style_Transparent>true</Style_Transparent>

        <AutoStretchHorizontal>true</AutoStretchHorizontal>
        <LeftAnchorToLeft>true</LeftAnchorToLeft>
        <RightAnchorToLeft>false</RightAnchorToLeft>

        <Size>
            <CY>96</CY>
        </Size>

        <Pieces>LayoutBox:OW_ALL_TitleImageLayout</Pieces>
        <Pieces>TileLayoutBox:OW_ALL_QuestStatsLayout</Pieces>
    </Screen>

</XML>